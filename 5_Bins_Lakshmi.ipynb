   "source": [
    "import pandas as pd\n",
    "\n",
    "df=pd.DataFrame([23.5,5,55,78.3],columns=['percentage'])\n",
    "\n",
    "bins = [0, 1, 5, 10, 25, 50, 100]\n",
    "labels = [1,2,3,4,5,6]\n",
    "df['binned'] = pd.cut(df['percentage'], bins=bins, labels=labels)\n",
    "print (df)"
   ]
